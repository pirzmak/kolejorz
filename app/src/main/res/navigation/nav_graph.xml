<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.chciabymbyckolejarzem.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_dwaFragment"
            app:destination="@id/dwaFragment" />
        <action
            android:id="@+id/action_mainFragment_to_jedenFragment"
            app:destination="@id/jedenFragment" />
    </fragment>
    <fragment
        android:id="@+id/dwaFragment"
        android:name="com.example.chciabymbyckolejarzem.DwaFragment"
        android:label="fragment_dwa"
        tools:layout="@layout/fragment_dwa" />
    <fragment
        android:id="@+id/jedenFragment"
        android:name="com.example.chciabymbyckolejarzem.jedenFragment"
        android:label="fragment_jeden"
        tools:layout="@layout/fragment_jeden" />
</navigation>